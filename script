----------------------------------------------3.카테고리(CATEGORY)-------------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE CATEGORY(
    CATEGORY_NO NUMBER CONSTRAINT CATN_PK PRIMARY KEY,
    CATEGORY_NAME VARCHAR2(30) CONSTRAINT CATN_NN NOT NULL
    );

COMMENT ON COLUMN CATEGORY.CATEGORY_NO IS '카테고리번호';
COMMENT ON COLUMN CATEGORY.CATEGORY_NAME IS '카테고리이름';

CREATE SEQUENCE SEQ_CATEGORY_NO NOCACHE;

INSERT INTO CATEGORY VALUES (SEQ_CATEGORY_NO.NEXTVAL, 'DSLR');
INSERT INTO CATEGORY VALUES (SEQ_CATEGORY_NO.NEXTVAL, 'SLR');
INSERT INTO CATEGORY VALUES (SEQ_CATEGORY_NO.NEXTVAL, '미러리스트');
INSERT INTO CATEGORY VALUES (SEQ_CATEGORY_NO.NEXTVAL, '임펙트');

------------------------------------------------------------------------------------------------------------
------------------------------------4.게시판(BOARD)------------------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE BOARD (
   BOARD_NO NUMBER CONSTRAINT BOARD_PK PRIMARY KEY,
   BOARD_TITLE VARCHAR2(100) NOT NULL,
   BOARD_CONTENT VARCHAR2(4000) NOT NULL,
   BOARD_WRITER NUMBER NOT NULL,
   COUNT NUMBER DEFAULT 0 NOT NULL,
       BOARD_TYPE NUMBER NOT NULL,
   STATUS   VARCHAR2(1) DEFAULT 'Y' CHECK (STATUS IN('Y', 'N')) NOT NULL,
   CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
   HASHTAG   VARCHAR2(100),

   FOREIGN KEY(BOARD_WRITER) REFERENCES MEMBER(USER_NO)
);


COMMENT ON COLUMN BOARD.BOARD_NO IS '게시글 번호';
COMMENT ON COLUMN BOARD.BOARD_TITLE IS '게시글 제목';
COMMENT ON COLUMN BOARD.BOARD_CONTENT IS '게시글 내용';
COMMENT ON COLUMN BOARD.BOARD_WRITER IS '회원번호';
COMMENT ON COLUMN BOARD.COUNT IS '조회수';
COMMENT ON COLUMN BOARD.BOARD_TYPE IS '게시판 타입';
COMMENT ON COLUMN BOARD.STATUS IS '게시글 상태';
COMMENT ON COLUMN BOARD.CREATE_DATE IS '게시글 작성일자';
COMMENT ON COLUMN BOARD.HASHTAG IS '해시태그';

CREATE SEQUENCE SEQ_BOARD_NO NOCACHE;


