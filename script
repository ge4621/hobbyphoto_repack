---------------------------------------------5.공지사항--------------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE NOTICE (
    NOTICE_NO VARCHAR2(5) PRIMARY KEY,
    NOTICE_WRITER NUMBER NOT NULL,
    NOTICE_TITLE VARCHAR2(1000) NOT NULL,
    NOTICE_CONTENT VARCHAR2(4000) NOT NULL,
    COUNT NUMBER DEFAULT 0 ,
    CREATE_DATE DATE DEFAULT SYSDATE ,
    STATUS VARCHAR2(1) DEFAULT 'Y' CHECK (STATUS IN('Y', 'N')) NOT NULL,

    FOREIGN KEY(NOTICE_WRITER) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN NOTICE.NOTICE_NO IS '공지사항 번호';
COMMENT ON COLUMN NOTICE.NOTICE_WRITER IS '작성자 번호';
COMMENT ON COLUMN NOTICE.NOTICE_TITLE IS '공지사항 제목';
COMMENT ON COLUMN NOTICE.NOTICE_CONTENT IS '공지사항 내용';
COMMENT ON COLUMN NOTICE.COUNT IS '조회수';
COMMENT ON COLUMN NOTICE.CREATE_DATE IS '작성일';
COMMENT ON COLUMN NOTICE.STATUS IS '상태값';

CREATE SEQUENCE SEQ_NOTIC_NO NOCACHE;

INSERT INTO NOTICE VALUES (SEQ_NOTIC_NO.NEXTVAL,1,'첫번째 공지사항입니다', '첫번째 공지입니다', 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTIC_NO.NEXTVAL,1, '두번째 공지사항입니다', '두번째 공지입니다', 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTIC_NO.NEXTVAL,1,'세번째 공지사항입니다', '세번째 공지입니다', 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTIC_NO.NEXTVAL,1,'네번째 공지사항입니다', '네번째 공지입니다', 0, SYSDATE,'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTIC_NO.NEXTVAL,1,'다섯번째 공지사항입니다', '다섯번째 공지입니다', 0, SYSDATE, 'Y');


