----------------------------------------------------------------------------------------------
---------------------------------------8.배경화면------------------------------------------------
----------------------------------------------------------------------------------------------
CREATE TABLE BACKGROUND(
	BACK_NO NUMBER CONSTRAINT BACK_PK PRIMARY KEY,
	USER_NO NUMBER NOT NULL,
	IMG VARCHAR2(500) NOT NULL,
	CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
	COUNT NUMBER DEFAULT 0 NOT NULL,
	DOWNLOAD NUMBER DEFAULT 0 NOT NULL,

	FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN BACKGROUND.BACK_NO IS '배경 게시글 번호';
COMMENT ON COLUMN BACKGROUND.USER_NO IS '작성자';
COMMENT ON COLUMN BACKGROUND.IMG IS '배경사진';
COMMENT ON COLUMN BACKGROUND.CREATE_DATE IS '배경 게시글 작성일';
COMMENT ON COLUMN BACKGROUND.COUNT IS '조회수';
COMMENT ON COLUMN BACKGROUND.DOWNLOAD IS '다운로드수';

CREATE SEQUENCE SEQ_BACK_NO NOCACHE;

------------------------------------------------------------------------------------------------------------
------------------------------------------9.상품(PRODUCT) ----------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE PRODUCT (
	P_NO NUMBER CONSTRAINT PROD_NO_PK PRIMARY KEY,
	P_NAME VARCHAR2(100) NOT NULL,
	CATEGORY_NO NUMBER CONSTRAINT CATNO_NN NOT NULL,
	BRAND_NO NUMBER CONSTRAINT PBRNO_NN NOT NULL,
	PRICE VARCHAR2(50) NOT NULL,
	AMOUNT NUMBER DEFAULT 0 NULL,
	STATUS VARCHAR2(1) DEFAULT 'Y' CHECK (STATUS IN('Y', 'N')) NOT NULL,
	THUMBNAIL VARCHAR2(500) NOT NULL,
	P_DIMG VARCHAR2(500) NOT NULL,
	P_SIMG VARCHAR2(500) NOT NULL,
	
	FOREIGN KEY(CATEGORY_NO) REFERENCES CATEGORY(CATEGORY_NO),
	FOREIGN KEY(BRAND_NO) REFERENCES BRAND(BRAND_NO)
);

COMMENT ON COLUMN PRODUCT.P_NO IS '상품번호';
COMMENT ON COLUMN PRODUCT.P_NAME IS '상품이름';
COMMENT ON COLUMN PRODUCT.CATEGORY_NO IS '카테고리 번호';
COMMENT ON COLUMN PRODUCT.BRAND_NO IS '브랜드번호';
COMMENT ON COLUMN PRODUCT.PRICE IS '상품 가격';
COMMENT ON COLUMN PRODUCT.AMOUNT IS '재고';
COMMENT ON COLUMN PRODUCT.STATUS IS '상태';
COMMENT ON COLUMN PRODUCT.THUMBNAIL IS '상품 썸네일';
COMMENT ON COLUMN PRODUCT.P_DIMG IS '상세내용이미지';
COMMENT ON COLUMN PRODUCT.P_SIMG IS '제품 상세이미지';

CREATE SEQUENCE SEQ_PRODUCT NOCACHE;

INSERT INTO PRODUCT
VALUES (SEQ_PRODUCT.NEXTVAL, 'CANON EOS R8 (Body)', 1, 1, '2059000', 10, DEFAULT,'APPLE.JPG','BANANA.JPG','ORANGE.JPG');

