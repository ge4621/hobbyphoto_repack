------------------------------------------------------------------------------------------------------------------
-----------------------------------------------------10.주문---------------------------------------------------------
------------------------------------------------------------------------------------------------------------------
CREATE TABLE ORDERS(
	ORDER_NO NUMBER CONSTRAINT ORDER_NO_PK PRIMARY KEY,
	USER_NO NUMBER,
	
	FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN ORDERS.ORDER_NO IS '주문상세번호';
COMMENT ON COLUMN ORDERS.USER_NO IS '회원번호';

CREATE SEQUENCE SEQ_ORDER_NO NOCACHE;

INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 1);
INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 3);
INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 2);
INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 2);
INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 3);
INSERT INTO ORDERS VALUES (SEQ_ORDER_NO.NEXTVAL, 4);

-----------------------------------------------------------------------------------------------------------
---------------------------------------------11.주문상세-----------------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE D_ORDER(
    D_ORDER_NO NUMBER CONSTRAINT DORDER_NO_PK PRIMARY KEY,
    ORDER_NO NUMBER NOT NULL,
    P_NO NUMBER NOT NULL,
    NAME VARCHAR2(15) NOT NULL,
    ADDRESS VARCHAR2(300) NOT NULL,
    POSTCODE NUMBER NOT NULL,
    PHONE VARCHAR2(15) NOT NULL,
    ORDER_DATE DATE NOT NULL,
    TOTALPRICE VARCHAR2(50) NOT NULL,
    STATUS VARCHAR2(1) DEFAULT 'Y' CHECK (STATUS IN('Y', 'N')) NOT NULL,
    P_TYPE VARCHAR2(10) NOT NULL,
    O_MSG VARCHAR2(4000),
    D_MSG VARCHAR2(4000),
	
    FOREIGN KEY(ORDER_NO) REFERENCES ORDERS(ORDER_NO),
    FOREIGN KEY(P_NO) REFERENCES PRODUCT(P_NO)
);

COMMENT ON COLUMN D_ORDER.D_ORDER_NO IS '주문상세번호';
COMMENT ON COLUMN D_ORDER.ORDER_NO IS '주문번호';
COMMENT ON COLUMN D_ORDER.P_NO IS '상품번호';
COMMENT ON COLUMN D_ORDER.NAME IS '이름';
COMMENT ON COLUMN D_ORDER.ADDRESS IS '배송지';
COMMENT ON COLUMN D_ORDER.POSTCODE IS '우편번호';
COMMENT ON COLUMN D_ORDER.PHONE IS '휴대폰번호';
COMMENT ON COLUMN D_ORDER.ORDER_DATE IS '주문날짜';
COMMENT ON COLUMN D_ORDER.TOTALPRICE IS '가격';
COMMENT ON COLUMN D_ORDER.STATUS IS '상태';
