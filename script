---------------------------------------2.브랜드(BRAND)--------------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE BRAND (
	BRAND_NO NUMBER	CONSTRAINT BRAND_PK PRIMARY KEY,
	BRAND_NAME VARCHAR2(100) NOT NULL
);

COMMENT ON COLUMN BRAND.BRAND_NO IS '브랜드번호';
COMMENT ON COLUMN BRAND.BRAND_NAME IS '브랜드명';

CREATE SEQUENCE SEQ_BRNO_NO NOCACHE;

