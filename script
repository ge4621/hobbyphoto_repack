------------------------------------------------------------------------------------------------------------
-----------------------------------------18.소모임(S_GROUP)----------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE S_GROUP(
   GROUP_NO NUMBER CONSTRAINT SGROUP_PK PRIMARY KEY,
   IMG VARCHAR2(1000) NOT NULL,
   USER_NO   NUMBER   NOT NULL,
   TITLE VARCHAR2(1000),
   CONTENT VARCHAR2(4000),
   ME_TOTAL NUMBER,
   ME_COUNT NUMBER DEFAULT 1,
   LOCATION VARCHAR2(4000),
   GR_DATE DATE,
   CREATE_DATE DATE,
   STATUS VARCHAR2(10),
   BOARD_TYPE VARCHAR2(1)
   
);

COMMENT ON COLUMN S_GROUP.GROUP_NO IS '소모임 번호';
COMMENT ON COLUMN S_GROUP.IMG IS '대표 이미지';
COMMENT ON COLUMN S_GROUP.USER_NO IS '모임 만든 회원';
COMMENT ON COLUMN S_GROUP.TITLE IS '모임제목';
COMMENT ON COLUMN S_GROUP.CONTENT IS '모임 내용';
COMMENT ON COLUMN S_GROUP.ME_TOTAL IS '모임인원';
COMMENT ON COLUMN S_GROUP.LOCATION IS '모임위치';
COMMENT ON COLUMN S_GROUP.GR_DATE IS '모임 날짜';
COMMENT ON COLUMN S_GROUP.CREATE_DATE IS '게시글 생성날짜';
COMMENT ON COLUMN S_GROUP.STATUS IS '상태값';
COMMENT ON COLUMN S_GROUP.BOARD_TYPE IS '게시글 타입(3)';
COMMENT ON COLUMN S_GROUP.ME_COUNT IS '신청자 인원';

CREATE SEQUENCE SEQ_SGR NOCACHE;

