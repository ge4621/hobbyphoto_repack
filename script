-------------------------------------------------13.첨부파일(게시판)----------------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE ATTACHMENT(
    FILE_NO NUMBER CONSTRAINT FNNO_PK PRIMARY KEY,
    FILE_LEVEL NUMBER CONSTRAINT FILE_LEVLE NOT NULL,
    ORIGIN_NAME VARCHAR2(200) CONSTRAINT ORN_NN NOT NULL,
    CHANGE_NAME VARCHAR2(200) CONSTRAINT CHN_NN NOT NULL,
    FILE_PATH VARCHAR2(1000) CONSTRAINT FIP_NN NOT NULL,
    UPLOAD_DATE DATE DEFAULT SYSDATE CONSTRAINT UPD_NN NOT NULL,
    STATUS VARCHAR2(1) DEFAULT 'Y' CONSTRAINT ATS_NN NOT NULL CHECK(STATUS IN('Y','N')),
    REF_BNO NUMBER CONSTRAINT RB_NN NOT NULL
    );

COMMENT ON COLUMN ATTACHMENT.FILE_NO IS '파일번호';
COMMENT ON COLUMN ATTACHMENT.FILE_LEVEL IS '파일레벨';
COMMENT ON COLUMN ATTACHMENT.ORIGIN_NAME IS '파일원본명';
COMMENT ON COLUMN ATTACHMENT.CHANGE_NAME IS '파일수정명';
COMMENT ON COLUMN ATTACHMENT.FILE_PATH IS '저장경로';
COMMENT ON COLUMN ATTACHMENT.UPLOAD_DATE IS '업로드일';
COMMENT ON COLUMN ATTACHMENT.STATUS IS '상태값';
COMMENT ON COLUMN ATTACHMENT.REF_BNO IS '참조게시글번호';

CREATE SEQUENCE SEQ_FILE_NO NOCACHE;


