-------------------------------------25.케이스--------------------------------------------------------
--------------------------------------------------------------------------------------------------
CREATE TABLE CASE(
	C_NO NUMBER CONSTRAINT CASE_PK PRIMARY KEY,
	USER_NO NUMBER NOT NULL,
	PHOTO VARCHAR2(500) NOT NULL,
	PRICE NUMBER NOT NULL,
	KIND VARCHAR2(1000) NOT NULL,
	CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
	STATUS VARCHAR2(1) DEFAULT 'B' CHECK (STATUS IN('B', 'I' ,'F')) NOT NULL,

	FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN CASE.C_NO IS '케이스 번호';
COMMENT ON COLUMN CASE.USER_NO IS '작성자';
COMMENT ON COLUMN CASE.PHOTO IS '사진';
COMMENT ON COLUMN CASE.PRICE IS '가격';
COMMENT ON COLUMN CASE.KIND IS '케이스 종류';
COMMENT ON COLUMN CASE.CREATE_DATE IS '작성일자';
COMMENT ON COLUMN CASE.STATUS IS '시작 전:B,진행:I,완료:F';

CREATE SEQUENCE SEQ_CASE_NO NOCACHE;

INSERT INTO CASE (C_NO, USER_NO, PHOTO, PRICE, KIND,CREATE_DATE, STATUS)
VALUES (SEQ_CASE_NO.NEXTVAL, 2 , 'profile2.jpg', 10000 , '갤럭시' , DEFAULT , DEFAULT);

INSERT INTO CASE (C_NO, USER_NO, PHOTO, PRICE, KIND,CREATE_DATE, STATUS)
VALUES (SEQ_CASE_NO.NEXTVAL, 3 , 'profile3.jpg', 5000 , '아이폰' , DEFAULT , DEFAULT);




COMMIT;
