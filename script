----------------------------------------------22.좋아요(LIKES)--------------------------------------------------------
-----------------------------------------------------------------------------------------------------------
CREATE TABLE LIKES(
      BOARD_NO NUMBER,
      USER_NO NUMBER,
      BOARD_TYPE NUMBER CONSTRAINT BTYPE_NN NOT NULL,
      PRIMARY KEY(BOARD_NO,USER_NO,BOARD_TYPE)
    );

COMMENT ON COLUMN LIKES.BOARD_NO IS '게시글번호';
COMMENT ON COLUMN LIKES.USER_NO IS '회원번호';
COMMENT ON COLUMN LIKES.BOARD_TYPE IS '게시글 타입';

--CREATE SEQUENCE SEQ_LIKES_NO NOCACHE;

