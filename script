CREATE TABLE REPORT(
   RP_NO NUMBER CONSTRAINT RP_NO_PK PRIMARY KEY,
   GUILTY VARCHAR2(100),
   RP_CONTENT VARCHAR2(1000) CONSTRAINT RP_CONTENT_NN NOT NULL,
   PROCESS VARCHAR2(1) DEFAULT 'N',
   REF_BNO VARCHAR2(5) CONSTRAINT REF_BNO_NN NOT NULL,
   REPORT_UNO NUMBER CONSTRAINT REPORT_UNO_NN NOT NULL,
   REPORTED_UNO NUMBER CONSTRAINT REPORTED_UNO_NN NOT NULL,
   BOARD_TYPE VARCHAR2(100), 

   FOREIGN KEY (REPORT_UNO) REFERENCES MEMBER,
   FOREIGN KEY (REPORTED_UNO) REFERENCES MEMBER
);

COMMENT ON COLUMN REPORT.RP_NO IS '신고번호';
COMMENT ON COLUMN REPORT.GUILTY IS '혐의';
COMMENT ON COLUMN REPORT.RP_CONTENT IS '내용';
COMMENT ON COLUMN REPORT.PROCESS IS '처리여부';
COMMENT ON COLUMN REPORT.REF_BNO IS '참조게시글번호';
COMMENT ON COLUMN REPORT.REPORT_UNO IS '신고자';
COMMENT ON COLUMN REPORT.REPORTED_UNO IS '피신고자';
COMMENT ON COLUMN REPORT.BOARD_TYPE IS '게시글 타입';

