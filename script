------------------------------------------------------------------------------------------------------------------
-----------------------------------------------------10.주문---------------------------------------------------------
------------------------------------------------------------------------------------------------------------------
CREATE TABLE ORDERS(
   ORDER_NO NUMBER CONSTRAINT ORDER_NO_PK PRIMARY KEY,
   USER_NO NUMBER,
   NAME VARCHAR2(15) NOT NULL,
   ADDRESS VARCHAR2(300) NOT NULL,
   POSTCODE VARCHAR2(300) NOT NULL,
   PHONE VARCHAR2(15) NOT NULL,
   ORDER_DATE DATE DEFAULT SYSDATE NOT NULL,
   TOTALPRICE VARCHAR2(50) NOT NULL,
   STATUS VARCHAR2(50) DEFAULT '상품준비' CHECK (STATUS IN('상품준비', '배송중','배송완료','구매확정')) NOT NULL,
   O_MSG VARCHAR2(4000),
   D_MSG VARCHAR2(4000), 

   FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN ORDERS.ORDER_NO IS '주문번호';
COMMENT ON COLUMN ORDERS.USER_NO IS '회원번호';
COMMENT ON COLUMN ORDERS.NAME IS '받는사람';
COMMENT ON COLUMN ORDERS.ADDRESS IS '배송지';
COMMENT ON COLUMN ORDERS.POSTCODE IS '상세주소';
COMMENT ON COLUMN ORDERS.PHONE IS '휴대폰번호';
COMMENT ON COLUMN ORDERS.ORDER_DATE IS '주문날짜';
COMMENT ON COLUMN ORDERS.TOTALPRICE IS '가격';
COMMENT ON COLUMN ORDERS.STATUS IS '상태(상품준비,배송중,배송완료,구매확정)';
COMMENT ON COLUMN ORDERS.O_MSG IS '주문메세지';
COMMENT ON COLUMN ORDERS.D_MSG IS '배송메세지';

CREATE SEQUENCE SEQ_ORDER_NO NOCACHE;


