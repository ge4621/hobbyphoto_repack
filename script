-------------------------------23.포토북 템플릿(TEMPLATES)-----------------------------------------------
-----------------------------------------------------------------------------------------
CREATE TABLE TEMPLATES(
   T_NO NUMBER CONSTRAINT TNO_PK PRIMARY KEY,
   TEM_NAME VARCHAR2(100) NOT NULL,
   PRICE NUMBER NOT NULL,
   P_TYPE VARCHAR2(30) DEFAULT 2 NOT NULL,
   TITLE_IMG VARCHAR2(4000) NOT NULL,
   TEM_IMG VARCHAR2(4000) NOT NULL,
   STATUS VARCHAR2(1) DEFAULT 'Y' CHECK (STATUS IN('Y', 'N')) NOT NULL
);

COMMENT ON COLUMN TEMPLATES.T_NO IS '템플릿 번호';
COMMENT ON COLUMN TEMPLATES.TEM_NAME IS '템플릿 이름';
COMMENT ON COLUMN TEMPLATES.PRICE IS '템플릿 가격';
COMMENT ON COLUMN TEMPLATES.P_TYPE IS '상품 구분(2)';
COMMENT ON COLUMN TEMPLATES.TITLE_IMG IS '템플릿 이미지';
COMMENT ON COLUMN TEMPLATES.TEM_IMG IS '템플릿 세부 이미지';
COMMENT ON COLUMN TEMPLATES.STATUS IS '템플릿상태';
CREATE SEQUENCE SEQ_TEM_NO NOCACHE;

