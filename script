--------------------------------------24.포토북-------------------------------------------------
--------------------------------------------------------------------------------------------
CREATE TABLE PHOTO(
	P_NO NUMBER CONSTRAINT PNO_PK PRIMARY KEY,
	T_NO NUMBER NOT NULL,
	USER_NO NUMBER NOT NULL,
	IMG1 VARCHAR2(1000) NOT NULL,
	IMG2 VARCHAR2(1000) NOT NULL,
	IMG3 VARCHAR2(1000) NOT NULL,
	IMG4 VARCHAR2(1000) NOT NULL,
	IMG5 VARCHAR2(1000) NOT NULL,
	IMG6 VARCHAR2(1000) NOT NULL,
	IMG7 VARCHAR2(1000) NOT NULL,
	IMG8 VARCHAR2(1000) NOT NULL,
	IMG9 VARCHAR2(1000) NOT NULL,
	IMG0 VARCHAR2(1000) NOT NULL,
	CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
	STATUS VARCHAR2(1) DEFAULT 'B' CHECK (STATUS IN('B', 'I' ,'F')) NOT NULL,

	FOREIGN KEY(T_NO) REFERENCES TEMPLATES(T_NO),
	FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN PHOTO.P_NO IS '포토북 번호';
COMMENT ON COLUMN PHOTO.T_NO IS '템플릿 번호';
COMMENT ON COLUMN PHOTO.USER_NO IS '작성자';
COMMENT ON COLUMN PHOTO.IMG1 IS '사진1';
COMMENT ON COLUMN PHOTO.IMG2 IS '사진2';
COMMENT ON COLUMN PHOTO.IMG3 IS '사진3';
COMMENT ON COLUMN PHOTO.IMG4 IS '사진4';
COMMENT ON COLUMN PHOTO.IMG5 IS '사진5';
COMMENT ON COLUMN PHOTO.IMG6 IS '사진6';
COMMENT ON COLUMN PHOTO.IMG7 IS '사진7';
COMMENT ON COLUMN PHOTO.IMG8 IS '사진8';
COMMENT ON COLUMN PHOTO.IMG9 IS '사진9';
COMMENT ON COLUMN PHOTO.IMG0 IS '사진10';
COMMENT ON COLUMN PHOTO.CREATE_DATE IS '작성일자';
COMMENT ON COLUMN PHOTO.STATUS IS '시작 전:B,진행:I,완료:F';

CREATE SEQUENCE SEQ_PHOTO_NO NOCACHE;

