CREATE TABLE CATEGORY(
    CATEGORY_NO NUMBER CONSTRAINT CATN_PK PRIMARY KEY,
    CATEGORY_NAME VARCHAR2(30) CONSTRAINT CATN_NN NOT NULL
    );

COMMENT ON COLUMN CATEGORY.CATEGORY_NO IS '카테고리번호';
COMMENT ON COLUMN CATEGORY.CATEGORY_NAME IS '카테고리이름';

CREATE SEQUENCE SEQ_CATEGORY_NO NOCACHE;

