INSERT INTO NOTICE VALUES (SEQ_NOTICE_NO.NEXTVAL,1,'다섯번째 공지사항입니다', '다섯번째 공지입니다', 1, 0, SYSDATE, 'Y');

INSERT INTO NOTICE VALUES (SEQ_NOTICE_NO.NEXTVAL,1,'쇼핑몰 첫번째 공지사항입니다', '첫번째 공지입니다', 2, 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTICE_NO.NEXTVAL,1, '쇼핑몰 두번째 공지사항입니다', '두번째 공지입니다',2, 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTICE_NO.NEXTVAL,1,'쇼핑몰 세번째 공지사항입니다', '세번째 공지입니다', 2, 0, SYSDATE, 'Y');
INSERT INTO NOTICE VALUES (SEQ_NOTICE_NO.NEXTVAL,1,'쇼핑몰 네번째 공지사항입니다', '네번째 공지입니다', 2, 0, SYSDATE,'Y');
