-------------------------------------26.팔로우(FOLLOW)--------------------------------------------------------
--------------------------------------------------------------------------------------------------
CREATE TABLE FOLLOW(
    USER_ID VARCHAR2(100) NOT NULL,
    FOLLOW_ID VARCHAR2(100) NOT NULL,
    FOLLOW_DATE DATE DEFAULT SYSDATE NOT NULL,
    PRIMARY KEY(USER_ID,FOLLOW_ID)
);

COMMENT ON COLUMN FOLLOW.USER_ID IS '팔로워 아이디';
COMMENT ON COLUMN FOLLOW.FOLLOW_ID IS '팔로우 아이디';
COMMENT ON COLUMN FOLLOW.FOLLOW_DATE IS '팔로우 한 날짜';

INSERT INTO FOLLOW(USER_ID,FOLLOW_ID) VALUES('user01','user04');
