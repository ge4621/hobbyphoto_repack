--------------------------------------------------------------------------------------------
--------------------------------------24.포토북(PHOTO)-------------------------------------------------
--------------------------------------------------------------------------------------------
CREATE TABLE PHOTO(
   P_NO NUMBER CONSTRAINT PNO_PK PRIMARY KEY,
   T_NO NUMBER NOT NULL,
   USER_NO NUMBER NOT NULL,
   CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
   STATUS VARCHAR2(1) DEFAULT 'N' CHECK (STATUS IN('Y', 'N')) NOT NULL,
   COUNT NUMBER NOT NULL,

   FOREIGN KEY(T_NO) REFERENCES TEMPLATES(T_NO),
   FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN PHOTO.P_NO IS '포토북 번호';
COMMENT ON COLUMN PHOTO.T_NO IS '템플릿 번호';
COMMENT ON COLUMN PHOTO.USER_NO IS '작성자';
COMMENT ON COLUMN PHOTO.CREATE_DATE IS '작성일자';
COMMENT ON COLUMN PHOTO.STATUS IS '진행중:N,완료:Y';
COMMENT ON COLUMN PHOTO.COUNT IS '포토수량';

CREATE SEQUENCE SEQ_PHOTO_NO NOCACHE;

------------------------------------------------------------------------------------------------
-------------------------------------24-1.포토북 상세(PHOTODETAIL)--------------------------------
-------------------------------------------------------------------------------------------------
CREATE TABLE PHOTODETAIL(
   P_NO NUMBER NOT NULL,
   T_NO NUMBER NOT NULL,
   USER_NO NUMBER NOT NULL,
   IMGS VARCHAR2(4000) NOT NULL,
   CREATE_DATE DATE DEFAULT SYSDATE NOT NULL,
   STATUS VARCHAR2(1) DEFAULT 'Y',

   FOREIGN KEY(P_NO) REFERENCES PHOTO(P_NO),
   FOREIGN KEY(T_NO) REFERENCES TEMPLATES(T_NO),
   FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO)
);

COMMENT ON COLUMN PHOTODETAIL.P_NO IS '포토북 번호';
COMMENT ON COLUMN PHOTODETAIL.T_NO IS '템플릿 번호';
COMMENT ON COLUMN PHOTODETAIL.USER_NO IS '작성자';
COMMENT ON COLUMN PHOTODETAIL.IMGS IS '사진';
COMMENT ON COLUMN PHOTODETAIL.CREATE_DATE IS '작성일자';
COMMENT ON COLUMN PHOTODETAIL.STATUS IS '완료:Y';

