   BOARD_NO NUMBER,
   USER_NO NUMBER,
   BMK_DATE DATE   DEFAULT SYSDATE   NULL,
   TYPE NUMBER NOT NULL,
   PRIMARY KEY(BOARD_NO,USER_NO,TYPE)
);

