-------------------------------------27.알람(ALRAM)--------------------------------------------------------
--------------------------------------------------------------------------------------------------
CREATE TABLE ALARM(
    ALARM_NO NUMBER PRIMARY KEY,
    TO_ID VARCHAR(20) NOT NULL,
    FROM_ID VARCHAR(20) NOT NULL,
    B_NO NUMBER NOT NULL,
    TITLE VARCHAR(100),
    CMD VARCHAR(20) NOT NULL,
    TYPE NUMBER DEFAULT 1,
    CREATE_DATE DATE DEFAULT SYSDATE NOT NULL
);

CREATE SEQUENCE SEQ_ALARM_NO NOCACHE;

COMMENT ON COLUMN ALARM.ALARM_NO IS '알람 번호';
COMMENT ON COLUMN ALARM.TO_ID IS '보내는사람 아이디';
COMMENT ON COLUMN ALARM.FROM_ID IS '받는사람 아이디';
COMMENT ON COLUMN ALARM.B_NO IS '참조 게시판 번호';
COMMENT ON COLUMN ALARM.TITLE IS '참조 게시판 제목';
COMMENT ON COLUMN ALARM.CMD IS '알람 종류';
COMMENT ON COLUMN ALARM.TYPE IS '참조게시판 타입';
COMMENT ON COLUMN ALARM.CREATE_DATE IS '생성일자';
COMMIT; 
