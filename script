------------------------------------------19.소모임-상세(GROUP_D)--------------------------------------------
------------------------------------------------------------------------------------------------------------
CREATE TABLE GROUP_D (
	GROUP_NO NUMBER	NOT NULL,
	USER_NO	NUMBER	NOT NULL,

	FOREIGN KEY(USER_NO) REFERENCES MEMBER(USER_NO),
	FOREIGN KEY(GROUP_NO) REFERENCES S_GROUP(GROUP_NO)
);

COMMENT ON COLUMN GROUP_D.GROUP_NO IS '소모임번호';
COMMENT ON COLUMN GROUP_D.USER_NO IS '참가회원번호';


