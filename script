----------------------------------------------15.리뷰----------------------------------------------------------
-----------------------------------------------------------------------------------------------------------
CREATE TABLE REVIEW(
    REVIEW_NO NUMBER CONSTRAINT REN_PK PRIMARY KEY,
    REF_PNO NUMBER CONSTRAINT REFP_NN NOT NULL,
    REVIEW_CONTENT VARCHAR2(1000) CONSTRAINT REC_NN NOT NULL,
    REVIEW_WRITER NUMBER CONSTRAINT REVW_NN NOT NULL,
    REVIEW_IMG VARCHAR2(1000) CONSTRAINT REVI_NN NOT NULL,
    CREATE_DATE DATE DEFAULT SYSDATE CONSTRAINT CRED_NN NOT NULL,
    STATUS VARCHAR2(1) DEFAULT 'Y' CONSTRAINT RST_NN NOT NULL CHECK(STATUS IN('Y','N')),
    
    FOREIGN KEY(REF_PNO) REFERENCES PRODUCT(P_NO),
    FOREIGN KEY(REVIEW_WRITER) REFERENCES MEMBER(USER_NO)
   );

COMMENT ON COLUMN REVIEW.REVIEW_NO IS '리뷰번호';
COMMENT ON COLUMN REVIEW.REF_PNO IS '상품번호';
COMMENT ON COLUMN REVIEW.REVIEW_CONTENT IS '리뷰내용';
COMMENT ON COLUMN REVIEW.REVIEW_WRITER IS '리뷰작성자';
COMMENT ON COLUMN REVIEW.REVIEW_IMG IS '리뷰이미지';
COMMENT ON COLUMN REVIEW.CREATE_DATE IS '작성일';
COMMENT ON COLUMN REVIEW.STATUS IS '상태';

CREATE SEQUENCE SEQ_REVIEW_NO NOCACHE;

